<form action="/crearEquipo" method="POST">
  <div class="form-group">
    <label>Nombre del Equipo</label>
    <input type="text" class="form-control" placeholder="Ingrese el nombre del Equipo" name="nombreEquipo" required>
  </div>
  <div class="form-group">
    <label>Fecha de creación</label>
    <input type="date" class="form-control" required name="fechaCreacion">
  </div>
  <div class="form-group">
    <label>País de residencia </label>
    <input type="text" class="form-control" placeholder="Ingrese el pais de residencia del Equipo" required name="paisResidencia">
  </div>
  <button type="submit" class="btn btn-success btn-block">Crear Equipo</button>
</form>
